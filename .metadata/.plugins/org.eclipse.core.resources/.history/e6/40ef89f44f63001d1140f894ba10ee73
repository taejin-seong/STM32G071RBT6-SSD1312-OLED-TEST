    delay(1000);
    */

    /*
    gpioPinWrite(_DEF_LED1, _DEF_HIHG);
    delay(100);
    gpioPinWrite(_DEF_LED1, _DEF_LOW);
    delay(100);
    */

    ssd1306_Fill(White);

  }
}


